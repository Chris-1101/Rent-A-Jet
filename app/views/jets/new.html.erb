
<h1 class="header_form">Add your jet</h1>
<div class="col-xs-12 col-sm-6 col-sm-offset-3">

  <%= simple_form_for(@jet) do |f|%>
  <div class="row_form">

  <div class="col-xs-12 col-sm-4-offset-3">


  <%= f.input :name %>
  <%= f.input :model %>
  <%= f.input :price_hour %>
  <%= f.input :seats %>

<html>
<body>
<form action="upload.php" method="post" enctype="multipart/form-data">
<input type="file" name="fileToUpload" id="fileToUpload"><!-- <input type="submit" value="Upload Image" name="submit"> -->
</form>
</body>

<%= f.submit class: "blue-button" %>
<%= link_to '', jets_path %>

<% end %>
</div>
</div>
</div>

