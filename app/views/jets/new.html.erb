
<h1 class="header_form">Add your jet</h1>
<div class="col-xs-12 col-sm-6 col-sm-offset-3">
  <%= simple_form_for(@jet) do |f|%>
    <div class="row_form">
      <div class="col-xs-12 col-sm-4-offset-3">
      <%= f.input :name %>
      <%= f.input :model %>
      <%= f.input :price_hour %>
      <%= f.input :city%>
      <%= f.input :seats %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.submit class: "blue-button" %>

</div>
    </div>
  <% end %>
</div>

